<%- include('../partials/head.ejs') %>

<div class="container">
    <h3 class="textTitle">Zaloguj się</h3>
    <form id="loginForm" method="post">
        <div class="form-group">   
            <label for="email">Wpisz swój E-mail</label>
            <div class="col-sm-10">
            <input type="email" placeholder="E-mail" id="email"/>
            </div>
            <label for="password">Wpisz hasło</label>
            <div class="col-sm-10">
            <input type="password" placeholder="Hasło" id="password"/>
            </div>
        </div>
        <div class="form-group">  
            <div class="col-sm-offset-2 col-sm-10">
            <button id="btn" type="submit">Zaloguj się</button>
            </div>
        </div>
</div>
<%- include('../partials/footer.ejs') %>